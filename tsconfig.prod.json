{
   "compilerOptions": {
       "module": "commonjs",
       "target": "es6",
       "outDir": "out",
       "lib": [
           "es6"
       ],
       "sourceMap": false,
       "removeComments": true,
       "rootDir": ".",
       "resolveJsonModule": true
   },
   "include": [
       "src/**/*.ts",
       "src/docs/**/*.json",
   ],
   "exclude": [
       "node_modules",
       ".vscode-test"
   ]
}